.text{text-align: center;font-size: 40px;height: 60px;line-height: 60px}

.weui-input {
    height: 2.58823529em;
    min-height: 2.58823529em;
    line-height: 2.58823529em;
    border: 1px solid #aaa;
    border-radius: 10px;
    padding: 0 10px;
}
.weui-input2 {
    border-bottom: 1px solid #aaa;
